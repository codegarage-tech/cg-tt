<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <gradient android:angle="0" android:endColor="#FAA732" android:startColor="#FAA732" />
            <stroke android:width="1dp" android:color="#444" />
            <!-- <stroke
                android:width="1dp"
                android:color="#F89406" /> -->
            <corners android:radius="0dp" />
            <padding android:bottom="0dp" android:left="0dp" android:right="0dp" android:top="0dp" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <gradient android:angle="0" android:endColor="#F89406" android:startColor="#FAA732" />
            <stroke android:width="0dp" android:color="#F89406" />
            <corners android:radius="0dp" />
            <padding android:bottom="0dp" android:left="0dp" android:right="0dp" android:top="0dp" />
        </shape>
    </item>
</selector>